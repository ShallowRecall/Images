{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "upimg.php", "use": "@vercel/php" }
  ],

  "routes": [
    { "src": "/", "dest": "index.html" },
    { "src": "/api/upimg", "dest": "upimg.php" }
  ],

  "env": {
    "PHP_EXTENSIONS": "gd" 
  },

  "ignores": ["images"]
}
